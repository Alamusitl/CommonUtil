<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 默认状态  -->
    <item android:drawable="@drawable/upomp_bypay_btn_esc_bg_click"
          android:state_window_focused="false"/>
    <!-- 按下状态  -->
    <item android:drawable="@drawable/upomp_bypay_btn_esc_bg" android:state_focused="true"
          android:state_pressed="true"/>
    <item android:drawable="@drawable/upomp_bypay_btn_esc_bg" android:state_focused="false"
          android:state_pressed="true"/>
    <!-- 选中状态  -->
    <item android:drawable="@drawable/upomp_bypay_btn_esc_bg"
          android:state_selected="true"/>
    <item android:drawable="@drawable/upomp_bypay_btn_esc_bg"
          android:state_focused="true"/>
</selector> 